{
	"plugins": ["plugins/markdown"],
	"recurseDepth": 10,
	"source": {
		"include": [
			"./src/actions/",
			"./src/common/",
			"./src/components/Header",
			"./src/components/Loader",
			"./src/components/Quote",
			"./src/components/Note",
			"./src/pages/Home",
			"./src/pages/Landscape",
			"./src/pages/Login",
			"./src/pages/Signup",
			"./src/reducers",
			"./src",
			"README.md"
		],
		"includePattern": ".+\\.js(doc|x)?$",
		"excludePattern": "node_modules"
	},
	"sourceType": "module",
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc", "closure"]
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false
	},
	"docdash": {
		"sort": true,
		"search": true,
		"collapse": false
	}
}
